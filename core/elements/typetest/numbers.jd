@type:shouldMatch(:int)
@type:shouldNotMatch(:string)
@type:shouldNotMatch(:11)
@type:shouldBe(:10) 10

@type:shouldMatch(:10 + 10) 20

@type:shouldNotMatch(:11 + 10) 20

@type:shouldMatch(:10 + 10) (10 + 10)

@type:shouldMatch(:int + 10) (10 + int)

@type:shouldNotMatch(:int)
@type:shouldMatch(:uint)
@type:shouldMatch(:uint + 10u)
@type:shouldNotMatch(:uint + 21u) 20u
