##[builtins]##
//:value:twoArgs:(one: int, two: string) => string
//:value:simple:() => string
//:value:takeSimple:(simple: () => string) => string
//:value:takeOneArg:(simple: (v: int) => string) => string
//:value:takeIntle:(simple: () => int) => string
//:value:oneArg:(v: int) => string

##[fixtures]##
==[well-typed builtin call]==
--[input]--
toString(1)
toString(3.4)

--[output:expected]--
alias string=h8f452146b75c912dedc5638091c55a088e30b7ee toString=hb906415c72c78c3b88d97d9c8adad6d0f4e89b02 toString_h2=h2868c60538d10c06eff3de97ea9a5627f06e8e9d
toString(1)
// string
toString_h2(3.4)
// string

==[multiple args]==
--[input]--
twoArgs(1, "hello")

--[output:expected]--
alias string=h8f452146b75c912dedc5638091c55a088e30b7ee twoArgs=h1dd55b3629241bfa7b281be99d7b03a6ca6317ae
twoArgs(1, "hello")
// string

==[pass lambda]==
--[input]--
takeSimple(simple)

--[output:expected]--
alias string=h8f452146b75c912dedc5638091c55a088e30b7ee takeSimple=h661bc82cf84ea9f8713e296fe170c24b03b7836f simple=hb184131ce065f6ea01a4dc5300b8971f0b1dde25
takeSimple(simple)
// string

==[pass wrong lambda]==
--[input]--
takeOneArg(simple)
takeIntle(simple)

--[output:expected]--
alias string=h8f452146b75c912dedc5638091c55a088e30b7ee takeOneArg=hb2ea0af87886a84aab2b910728dc4992597a3bfb simple=hb184131ce065f6ea01a4dc5300b8971f0b1dde25 error:argWrongType=hfcfe64cbbd9e081476f4c8c4a4b2f8f2f992804f int=h2f181c91131fd0daa282424fb3953604513df6fe takeIntle=h768fd053382ed07585296a431b4bfe84259e6e46
takeOneArg(@error:argWrongType(expected: :(v: int) => string) simple)
// string
takeIntle(@error:argWrongType(expected: :() => int) simple)
// string

==[arg type error]==
--[input]--
oneArg(true)

--[output:expected]--
alias string=h8f452146b75c912dedc5638091c55a088e30b7ee oneArg=hb08abf80591f2e867a9b48a1e552e95c835deae7 error:argWrongType=hfcfe64cbbd9e081476f4c8c4a4b2f8f2f992804f int=h2f181c91131fd0daa282424fb3953604513df6fe
oneArg(@error:argWrongType(expected: :int) true)
// string

==[wrong number of args]==
--[input]--
oneArg(1, 2)

--[output:expected]--
alias string=h8f452146b75c912dedc5638091c55a088e30b7ee oneArg=hb08abf80591f2e867a9b48a1e552e95c835deae7 error:wrongNumberOfArgs=he554807c644be72e6c4e5cb59aa0955f9ab67f13
@error:wrongNumberOfArgs() oneArg(1, 2)
// string

==[not a fn]==
