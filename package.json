{
    "name": "j2",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "esbuild --bundle devui/server.ts --platform=node --external:vite | node -",
        "grammar": "esbuild --bundle utils/build-grammar.ts --platform=node --sourcemap=inline | node --enable-source-maps -",
        "re-fixture": "esbuild --bundle utils/re-fixture.ts --platform=node --sourcemap=inline | node --enable-source-maps -",
        "-fix-ids": "esbuild --bundle utils/fix-ids.ts --platform=node --sourcemap=inline | node --enable-source-maps -",
        "fix-ids": "./utils/esrun.js utils/fix-ids.ts",
        "transform": "npm run sync; esbuild --bundle utils/generate-transform.ts --platform=node --sourcemap=inline | node --enable-source-maps - ./core/typed-ast.ts ./core/transform-tast.ts; prettier -w core/transform-tast.ts",
        "transform-ast": "npm run sync; esbuild --bundle utils/generate-transform.ts --platform=node --sourcemap=inline | node --enable-source-maps - ./core/grammar/base.parser.ts ./core/transform-ast.ts; prettier -w core/transform-ast.ts",
        "build": "npm run grammar; npm run transform; npm run transform-ast",
        "sync": "esbuild --bundle utils/sync-imports.ts --platform=node --sourcemap=inline | node --enable-source-maps -",
        "test": "esbuild --bundle core/tests/run.ts --platform=node --sourcemap=inline | node --enable-source-maps -",
        "nojest": "esbuild --bundle core/typing/__test__/analyze.nojest.ts --platform=node --sourcemap=inline | node --enable-source-maps -",
        "fixn": "env FIX=1 pnpm nojest",
        "fix": "env FIX=1 jest",
        "cov": "jest --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/core": "^7.18.5",
        "@babel/generator": "^7.18.2",
        "@babel/types": "^7.18.4",
        "@nextui-org/react": "1.0.0-beta.9",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@vitejs/plugin-react": "^1.3.2",
        "ansi-colors": "^4.1.3",
        "buffer": "^6.0.3",
        "fast-deep-equal": "^3.1.3",
        "object-hash": "^3.0.0",
        "peggy": "^2.0.1",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ts-jest": "^28.0.5",
        "typescript": "^4.7.3",
        "vite": "^2.9.12"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.17.12",
        "@types/babel__core": "^7.1.19",
        "@types/babel__generator": "^7.6.4",
        "@types/jest": "^28.1.1",
        "@types/node": "^17.0.42",
        "@types/object-hash": "^2.2.1",
        "esbuild": "^0.14.43",
        "jest": "^28.1.1"
    }
}
